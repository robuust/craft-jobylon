{
    "version": "1.1.1",
    "name": "robuust/craft-jobylon",
    "description": "Jobylon plugin for Craft CMS",
    "type": "craft-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Bob Olde Hampsink",
            "email": "bob@robuust.digital"
        }
    ],
    "extra": {
        "handle": "jobylon",
        "name": "Jobylon",
        "developer": "Robuust",
        "developerUrl": "https://robuust.digital",
        "hasCpSettings": false,
        "hasCpSection": false
    },
    "require": {
        "php": "^8.0",
        "craftcms/cms": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "robuust\\jobylon\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
